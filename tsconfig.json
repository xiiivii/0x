{
  "compilerOptions": {
    "strict": true,
    "target": "es6",
    "module": "esnext",
    "moduleResolution": "nodenext",
    "jsx": "react",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "removeComments": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": false,
    "noImplicitAny": false,
    "baseUrl": "./"
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules"]
}
